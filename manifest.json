{
  "manifest_version": 2,
  "name": "Anti-Phish",
  "description": "This extension will help prevent phishing attacks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "on-icon.png"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ],

  "background": {
    "scripts": ["background.js"]
  }

}